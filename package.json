{
  "name": "goat-loopback",
  "version": "0.1.0",
  "description": "Goat API server",
  "keywords": [
    "Goat",
    "loopback",
    "Form.io",
    "formiojs",
    "typescript"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "refresh": "npm run generate:api && npm run build && node .",
    "start": "npm run generate:api &&  npm run build && node .",
    "start:prod": "npm run generate:api &&  npm run build && pm2 start pm2.json --env production && pm2 logs",
    "stop:prod": "pm2 stop pm2.json --env production",
    "graphql": "openapi-to-graphql http://localhost:3000/openapi.json -p 4001",
    "generate:api": "node ./.goat/generate.ts",
    "test": "npm run build && NODE_ENV=test lb-mocha --allow-console-logs --recursive --delay --timeout 10000 \"dist/\" --exit",
    "build:apidocs": "lb-apidocs",
    "build": "rm -rf tsconfig.tsbuildinfo && lb-clean dist && mkdir ./dist && lb-tsc --target ES2019 --outDir ./dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "test:dev": "lb-mocha --allow-console-logs  dist/__tests__/**/*.js --exit",
    "migrate": "node ./migrate",
    "prepublishOnly": "npm run test",
    "pretest": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@goatlab/fluent": "^0.2.6",
    "@loopback/boot": "^2.1.1",
    "@loopback/context": "^3.1.0",
    "@loopback/core": "^2.1.0",
    "@loopback/openapi-v3": "^3.1.0",
    "@loopback/repository": "^2.2.1",
    "@loopback/rest": "^3.3.1",
    "@loopback/rest-explorer": "^2.1.1",
    "@loopback/service-proxy": "^2.1.1",
    "chokidar": "^3.3.0",
    "dotenv": "^8.2.0",
    "loopback-connector-mongodb": "^5.2.1",
    "loopback-datatype-objectid": "^1.0.5",
    "oasgraph-cli": "^0.4.3",
    "openapi-to-graphql-cli": "^1.6.3",
    "pm2": "^4.3.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@loopback/build": "^4.0.1",
    "source-map-support": "^0.5.16",
    "@loopback/testlab": "^3.1.1",
    "@types/node": "^10.17.17",
    "@typescript-eslint/parser": "^2.24.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@loopback/eslint-config": "^6.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "@loopback/tslint-config": "^2.1.0",
    "@types/express": "^4.17.2",
    "@types/express-serve-static-core": "^4.17.1",
    "@types/express-session": "^1.15.16",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.32",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "chai": "^4.2.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-watch": "^6.0.1",
    "jest": "^25.1.0",
    "mocha": "^7.1.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3",
    "watch": "^1.0.2"
  }
}
